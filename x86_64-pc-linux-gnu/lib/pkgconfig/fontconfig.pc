prefix=/root/legendcoin/depends/x86_64-pc-linux-gnu
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
sysconfdir=${prefix}/etc
localstatedir=${prefix}/var
PACKAGE=fontconfig
confdir=${sysconfdir}/fonts
cachedir=${localstatedir}/cache/${PACKAGE}

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.12.1
Requires:  freetype2
Requires.private:  expat
Libs: -L${libdir} -lfontconfig
Libs.private: -L/root/legendcoin/depends/x86_64-pc-linux-gnu/lib -lexpat   -L/root/legendcoin/depends/x86_64-pc-linux-gnu/lib -lfreetype    
Cflags: -I${includedir} -I/root/legendcoin/depends/x86_64-pc-linux-gnu/include   -I/root/legendcoin/depends/x86_64-pc-linux-gnu/include/freetype2    
